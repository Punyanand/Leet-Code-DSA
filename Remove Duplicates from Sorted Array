class Solution(object):
    def removeDuplicates(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        if not nums:
            return 0

        i = 0  # pointer for unique elements
        for j in range(1, len(nums)):
            if nums[j] != nums[i]:  # found new unique element
                i += 1
                nums[i] = nums[j]

        return i + 1
